{
    "name": "vs-prince",
    "displayName": "vs-prince",
    "description": "Create diagrams from python code",
    "version": "0.0.3",
    "publisher": "mhidvegi",
    "author": "Máté Hidvégi",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HMate/vs-prince.git"
    },
    "engines": {
        "vscode": "^1.53.0",
        "node": "22.15.1"
    },
    "categories": [
        "Visualization"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vs-prince.visualize-py-deps",
                "title": "Visualize python dependencies",
                "when": "editorLangId == python",
                "category": "VS-Prince",
                "icon": "$(circuit-board)"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "vs-prince.visualize-py-deps",
                    "when": "editorLangId == python",
                    "icon": "$(circuit-board)",
                    "group": "navigation"
                }
            ]
        }
    },
    "activationEvents": [
        "onWebviewPanel:princeViz"
    ],
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compileWebViewDev": "webpack --config ./build/webview.webpack.config.js --stats-error-details",
        "compileWebViewProduction": "webpack --mode production --config ./build/webview.webpack.config.js",
        "compileDevExtension": "webpack --config ./build/node-extension.webpack.config.js && node ./build/pyprince-prepare-script ./dist",
        "compileProductionExtension": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js && node ./build/pyprince-prepare-script ./dist",
        "compile": "npm run compileWebViewDev && npm run compileDevExtension",
        "nodemon": "nodemon --delay 3 --exec npm run compile --verbose",
        "package": "npm run compileWebViewProduction && npm run compileProductionExtension",
        "link-pyprince": "node build/link-pyprince.js",
        "lint": "eslint src --ext ts",
        "test:unit-test": "ts-mocha src/test/unit-tests/*.test.ts",
        "test:system-test": "wdio run ./wdio.conf.ts"
    },
    "extensionDependencies": [
        "ms-python.python"
    ],
    "devDependencies": {
        "@hpcc-js/wasm": "^2.13.1",
        "@mhidvegi/pyprince": "^0.0.4",
        "@svgdotjs/svg.draggable.js": "^3.0.2",
        "@svgdotjs/svg.js": "~3.2.0",
        "@svgdotjs/svg.panzoom.js": "^2.1.2",
        "@types/chai": "^5.2.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^25.0.3",
        "@types/opentype.js": "^1.3.1",
        "@types/vscode": "^1.107.0",
        "@types/vscode-webview": "^1.57.5",
        "@types/webpack": "^5.28.5",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "@vscode/python-extension": "^1.0.2",
        "@wdio/cli": "^9.18.4",
        "@wdio/globals": "^9.17.0",
        "@wdio/local-runner": "^9.18.4",
        "@wdio/mocha-framework": "^9.18.0",
        "@wdio/spec-reporter": "^9.18.0",
        "@wdio/types": "^9.19.1",
        "@wdio/visual-service": "^9.0.0",
        "chai": "^6.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^7.1.2",
        "elkjs": "^0.8.2",
        "eslint": "^7.15.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.5",
        "mini-css-extract-plugin": "2.9.4",
        "mocha": "^11.7.1",
        "nodemon": "^3.1.11",
        "opentype.js": "^1.3.3",
        "prettier": "^2.2.1",
        "resolve-url-loader": "^5.0.0",
        "sass-loader": "^16.0.6",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.4",
        "ts-mocha": "^11.1.0",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "tsx": "^4.20.3",
        "typescript": "^5.1.3",
        "typescript-plugin-css-modules": "^5.2.0",
        "url-loader": "^4.1.1",
        "wdio": "^6.0.1",
        "wdio-vscode-service": "^6.1.3",
        "wdio-wait-for": "^3.1.0",
        "web-worker": "^1.2.0",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1"
    },
    "overrides": {
        "wdio-vscode-service": {
            "webdriverio": "^9.2.11"
        }
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4
    }
}
