{
    "name": "vs-prince",
    "displayName": "vs-prince",
    "description": "Create Prince diagrams in vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vs-prince.visualize-py-deps",
                "title": "VS-Prince: Visualize python dependencies"
            }
        ],
        "keybindings": [
            {
                "command": "vs-prince.visualize-py-deps",
                "key": "ctrl+d"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack --config ./build/webview.webpack.config.js && webpack --config ./build/node-extension.webpack.config.js",
        "nodemon": "nodemon --delay 3 --exec npm run compile",
        "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
        "lint": "eslint src --ext ts",
        "test": "ts-mocha src/test/suite/*.test.ts"
    },
    "devDependencies": {
        "@svgdotjs/svg.draggable.js": "^3.0.2",
        "@svgdotjs/svg.js": "~3.2.0",
        "@svgdotjs/svg.panzoom.js": "^2.1.2",
        "@types/chai": "^4.2.22",
        "@types/glob": "^7.1.3",
        "@types/mini-css-extract-plugin": "^1.2.2",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/opentype.js": "^1.3.1",
        "@types/vscode": "^1.53.0",
        "@types/vscode-webview": "^1.57.1",
        "@types/webpack": "^4.41.26",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "chai": "^4.3.4",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^9.1.0",
        "css-loader": "^5.0.2",
        "eslint": "^7.15.0",
        "file-loader": "^6.2.0",
        "glob": "^7.1.6",
        "html-webpack-plugin": "^5.0.0",
        "mini-css-extract-plugin": "1.3.4",
        "mocha": "^8.1.3",
        "node-sass": "^6.0.1",
        "nodemon": "^2.0.22",
        "opentype.js": "^1.3.3",
        "prettier": "^2.2.1",
        "resolve-url-loader": "^3.1.2",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.16",
        "ts-mocha": "^8.0.0",
        "typescript": "^5.1.3",
        "typescript-plugin-css-modules": "^3.2.0",
        "url-loader": "^4.1.1",
        "vscode-test": "^1.4.1",
        "webpack": "^5.21.2",
        "webpack-cli": "^4.2.0"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4
    },
    "dependencies": {
        "elkjs": "^0.8.2",
        "web-worker": "^1.2.0"
    }
}
